<?xml version="1.0" encoding="ASCII"?>
<DbMddAndroid:DBScheme
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:DbMddAndroid="DbMddAndroid"
    xsi:schemaLocation="DbMddAndroid ../metamodel/DbMddAndroid.ecore"
    name="esquema2">
  <tables name="t1"
      outRelation="//@relations.0 //@relations.1">
    <columns
        name="c1"
        type="String"/>
  </tables>
  <tables name="t2"
      inRelation="//@relations.0">
    <columns
        name="c2"
        type="Integer"/>
  </tables>
  <tables name="t3"
      inRelation="//@relations.1">
    <columns
        name="c3"
        type="double"/>
    <columns
        name="c4"
        type="char"/>
  </tables>
  <relations
      source="//@tables.0"
      target="//@tables.1"
      maxSourceMultiplicity="1"/>
  <relations
      source="//@tables.0"
      target="//@tables.2"/>
</DbMddAndroid:DBScheme>
